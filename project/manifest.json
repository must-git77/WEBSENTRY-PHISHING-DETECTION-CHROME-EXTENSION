{
    "manifest_version": 3,
    "name": "WEBSENTRY: PHISHING DETECTION",
    "version": "0.1.0",
    "description": "A prototype ML-based Chrome extension for web security.",
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "tabs",
        "webNavigation"
    ],
    "host_permissions": [
        "http://127.0.0.1:5000/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "warning_styles.css"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}